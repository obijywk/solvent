{
  "name": "solvent",
  "version": "0.0.1",
  "description": "Puzzle solving toolbox",
  "author": {
    "name": "Matt Gruskin",
    "email": "matthew.gruskin@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/obijywk/solvent"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cropperjs": "^1.1.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.5",
    "@types/rewire": "^2.5.28",
    "@types/seedrandom": "^2.4.28",
    "chai": "^4.2.0",
    "cropperjs": "^1.5.1",
    "css-loader": "^2.1.1",
    "mocha": "^6.1.4",
    "rewire": "^4.0.1",
    "seedrandom": "^3.0.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.4.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^0.16.9",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "scripts": {
    "build-and-run": "npm run build-client && npm run build-native && npm run build-server && npm run run-server",
    "build-client": "webpack",
    "build-clue-database": "DEBUG=solvent nodejs built/server/lib/clue_database_builder.js",
    "build-native": "node-gyp configure && node-gyp build",
    "build-server": "tsc",
    "check-all": "npm run build-client && npm run build-native && npm run build-server && npm run lint-server && npm run test",
    "lint-server": "tslint -c tslint.json 'src/server/**/*.ts'",
    "run-server": "GOOGLE_APPLICATION_CREDENTIALS=credentials.json DEBUG=solvent nodejs built/server/server.js",
    "set-julia-mtimes": "python3 set_julia_mtimes.py",
    "set-julia-mtimes-and-run-server": "npm run set-julia-mtimes && npm run run-server",
    "test": "mocha --reporter spec --require ts-node/register 'src/**/*.test.ts'"
  },
  "dependencies": {
    "@google-cloud/vision": "^0.25.0",
    "@types/csv-parse": "^1.1.12",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/sqlite3": "^3.1.5",
    "csv-parse": "^4.4.1",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "node-addon-api": "^1.6.3",
    "sqlite3": "^4.0.6"
  },
  "gypfile": "true"
}
